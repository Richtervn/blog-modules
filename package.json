{
  "name": "blog-modules",
  "version": "2.0.0",
  "description": "my-blog",
  "main": "index.js",
  "scripts": {
    "client:dev": "cd client && npm run start",
    "server:dev": "cd server && npm run start",
    "client:build": "cd client && npm run build",
    "server:build": "cd server && npm run build",
    "client:serve": "cd client && serve -s build",
    "server:serve": "cd server && npm run serve",
    "start": "start npm run client:dev && start npm run server:dev",
    "build": "start npm run client:build && start npm run server:build",
    "serve": "start npm run server:serve && start npm run server:serve",
    "client-old": "cd client-old && npm run start"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxBracketSameLine": true
  },
  "author": "VuPham",
  "license": "ISC"
}
