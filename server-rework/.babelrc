{
  "presets": ["env", "stage-0"],
  "plugins": ["transform-async-to-generator",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "manifests": "./src/manifests"
      }
    }]
  ]
}